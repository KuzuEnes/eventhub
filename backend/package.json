{
  "name": "eventhub-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:prod": "node dist/src/main.js",
    "prisma:generate": "prisma generate --schema=prisma/schema.sqlite.prisma",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.sqlite.prisma",
    "prisma:studio": "prisma studio --schema=prisma/schema.sqlite.prisma",
    "seed": "ts-node prisma/seed.ts",
    "prisma:generate:sqlite": "prisma generate --schema=prisma/schema.sqlite.prisma",
    "prisma:migrate:sqlite": "prisma migrate dev --schema=prisma/schema.sqlite.prisma",
    "prisma:studio:sqlite": "prisma studio --schema=prisma/schema.sqlite.prisma",
    "seed:sqlite": "ts-node prisma/seed.ts",
    "prisma:generate:pg": "prisma generate --schema=prisma/schema.postgres.prisma",
    "prisma:migrate:pg": "prisma migrate dev --schema=prisma/schema.postgres.prisma",
    "prisma:deploy:pg": "prisma migrate deploy --schema=prisma/schema.postgres.prisma",
    "seed:pg": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "@prisma/client": "^5.0.0",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "prisma": "^5.0.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport": "^1.0.11"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
